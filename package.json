    "tailwindcss": "^1.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1"
  }
}
